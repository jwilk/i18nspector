ISO-8859-14 =
ISO-8859-15 =
KOI8-R =
KOI8-U =
KOI8-T = not-python
CP850 =
CP866 =
CP874 =
CP932 =
CP949 =
CP950 =
CP1250 =
CP1251 =
CP1252 =
CP1253 =
CP1254 =
CP1255 =
CP1256 =
CP1257 =
GB2312 =
EUC-JP =
EUC-KR =
EUC-TW = not-python
BIG5 =
BIG5-HKSCS =
GBK =
GB18030 =
SHIFT_JIS =
JOHAB =
TIS-620 =
VISCII = not-python
GEORGIAN-PS = not-python
UTF-8 =

[extra-aliases]
# This section lists aliases of encodings that are used by real-world PO files,
# but that are not known to Python.
